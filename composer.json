{
    "name": "rdev/wooga-enhanced-ecommerce",
    "description": "WWooGA-Enhanced-Ecommerce",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "type": "wordpress-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WoogaEnhancedEcommerce\\": "src/"
        }
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs"
    },
    "require": {
        "php": ">=7.1",
        "micropackage/requirements": "^1.0",
        "theiconic/php-ga-measurement-protocol": "^2.9"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "wp-coding-standards/wpcs": "*",
        "automattic/phpcs-neutron-ruleset": "^3.3",
        "phpcompatibility/phpcompatibility-wp": "^2.1"
    },
    "keywords": [
        "wordpress",
        "plugin",
        "boilerplate",
        "framework"
    ],
    "authors": [
        {
            "name": "Rakim09",
            "email": "bartlomiej.dziewa@outlook.com",
            "homepage": "https://github.com/Rakim09/"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
